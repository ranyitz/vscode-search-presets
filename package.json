{
  "name": "search-presets",
  "displayName": "Search Presets ðŸ”Ž",
  "description": "let you configure preset for vscode's search",
  "repository": {
    "url": "https://github.com/ranyitz/vscode-search-presets"
  },
  "version": "0.0.2",
  "publisher": "ranyitz",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:search-presets.search"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "search-presets.search",
        "title": "Search Presets: ðŸ”Ž Search"
      }
    ],
    "keybindings": [
      {
        "command": "search-presets.search",
        "key": "shift+alt+f",
        "mac": "shift+alt+f"
      }
    ],
    "configuration": {
      "title": "Search Presets",
      "properties": {
        "search-presets.custom": {
          "order": 1,
          "type": "object",
          "description": "Search Presets configuration",
          "additionalProperties": {
            "type": "object",
            "description": "preset object",
            "properties": {
              "filesToInclude": {
                "type": "string"
              },
              "filesToExclude": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.185",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "zod": "^3.19.1"
  }
}
